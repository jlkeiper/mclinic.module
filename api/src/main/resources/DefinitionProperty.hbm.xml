<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
	<class name="org.openmrs.module.mclinic.reporting.metadata.DefinitionProperty" table="mclinic_definition_property">
		<id name="id" type="int" column="id" unsaved-value="0">
			<generator class="native"/>
		</id>

		<discriminator column="id" insert="false"/>

		<property name="property" type="java.lang.String" column="property" length="255"/>

		<property name="propertyValue" type="java.lang.String" column="property_value" length="255"/>

		<property name="propertyDescription" type="java.lang.String" column="property_description" length="255"/>

		<many-to-one name="extendedDefinition" class="org.openmrs.module.mclinic.reporting.metadata.ExtendedDefinition">
			<column name="definition_id"/>
		</many-to-one>

		<many-to-one name="creator" class="org.openmrs.User" not-null="true">
			<column name="creator"/>
		</many-to-one>

		<property name="dateCreated" type="java.util.Date" column="date_created" not-null="true" length="19"/>

		<property name="retired" type="java.lang.Boolean" column="retired" length="1" not-null="true"/>

		<many-to-one name="retiredBy" class="org.openmrs.User">
			<column name="retired_by"/>
		</many-to-one>

		<property name="dateRetired" type="java.util.Date" column="date_retired" length="19"/>

		<property name="uuid" type="java.lang.String" column="uuid" length="38" unique="true"/>
	</class>
</hibernate-mapping>
